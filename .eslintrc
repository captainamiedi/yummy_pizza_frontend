{
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "plugins": ["import"],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "error",
      "import/first": "error",
      "react/prop-types": 0
    }
  }